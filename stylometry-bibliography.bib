@article{burrows_word-patterns_1987-2,
  title = {Word-{{Patterns}} and {{Story}}-{{Shapes}}: {{The Statistical Analysis}} of {{Narrative Style}}},
  volume = {2},
  issn = {0268-1145, 1477-4615},
  shorttitle = {Word-{{Patterns}} and {{Story}}-{{Shapes}}},
  doi = {10.1093/llc/2.2.61},
  abstract = {The thirty most common word-types in many English texts make up about two-fifths of all the word-tokens used in them. The respective frequencies of the word-types can be arrayed hierarchically to form a frequency profile for each text. When a number of profiles are correlated with each other, the resulting matrix shows appropriate resemblances and differences across the range of texts. The whole pattern of interrelationships can then be `mapped', by applying eigen analysis to the correlation-matrix.
This form of analysis can be applied to comparisons of dialogue with narrative; of the idiolects of different characters; or \textemdash{} in the territory to which the present paper is addressed \textemdash{} of narrative with narrative. When several narratives are compared, authorial and chronological determinants predominate. When the segments of a single narrative are compared, even subtler stylistic determinants declare themselves and point towards the possibility of a computerassisted literary criticism.},
  language = {en},
  timestamp = {2017-01-03T10:16:48Z},
